{
  "[notebook]": {
    "lineNumbers": "on",
    "output.textLineLimit": 200,
    "diff.ignoreOutputs": true
  }, 
  "[vim]": {
    "debug.loggingLevelForConsole": "debug",
  },
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>", "l"]
    }
  ],
  "tabnine.experimentalAutoImports": true,
  "files.associations": {
    "*.ejs": "html",
    "*.rmd": "markdown"
  },
  "[workbench]": {
    "colorTheme": "Oceanic Next (Sublime)",
    "startupEditor": "none"
  },
  "security.workspace.trust.untrustedFiles": "open",
  "yaml.schemas": {
    "file:///Users/tfarkas/.vscode/extensions/atlassian.atlascode-3.0.3/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "redhat.telemetry.enabled": false,
  "atlascode.bitbucket.enabled": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "typescript.suggest.paths": false,
  "javascript.suggest.paths": false,
  "[quarto]": {
    "editor.suggest.showSnippets": true,
    "editor.snippetSuggestions": "inline",
    "mathjax.theme": "dark"
  },
  "dvc.doNotShowCliUnavailable": true,
  "jupyter.askForKernelRestart": false,
  "[path-autocomplete]": {
    "disableUpOneFolder": false, 
    "pathMappings": {
      "workspace": "${workspace}"
    }
  },
  "[python]": {
    "editor.formatOnType": true,
    "analysis.autoFormatStrings": true
  },
  "[git]": {
    "ignoreMissingGitWarning": true,
    "autofetch": true,
    "terminalAuthentication": false
  },
  "[julia]": {
    "symbolCacheDownload": true,
    "enableTelemetry": false
  }, 
  "[terminal]": {
    "[integrated]": {
      "inheritEnv": false,
      "defaultProfile.linux": "zsh",
      "defaultProfile.osx": "zsh",
      "commandsToSkipShell": [
        "language-julia.interrupt"
      ], 
  //     "profiles.osx": {
  //       "tmux-shell": {
  //         "path": "tmux",
  //         "args": ["new-session", "-A", "-s", "vscode:${workspaceFolder}"]
  // }
  //     }
    }
  }, 
  "remote.SSH.connectTimeout": 60,
  "window.zoomLevel": 1,
  "vsicons.dontShowNewVersionMessage": true,
  "python.analysis.extraPaths": [],
  "github.copilot.enable": {
    "quarto": true,
    "*": true
  },
  "github.copilot.editor.enableAutoCompletions": true,
  "editor.fontFamily": "MesloLGS NF",
  "quarto.render.previewType": "external",
  "svelte.enable-ts-plugin": true,
  "editor.tabSize": 2,
  "editor.detectIndentation": false
}      

