{
  "[notebook]": {
    "lineNumbers": "on",
    "output.textLineLimit": 200,
    "diff.ignoreOutputs": true
  }, 
  "[vim]": {
    "debug.loggingLevelForConsole": "debug",
    "insertModeKeyBindings": [
      {
        "before": ["j", "k"],
        "after": ["<Esc>", "l"]
      }
    ]
  },
  "tabnine.experimentalAutoImports": true,
  "files.associations": {
    "*.ejs": "html",
    "*.rmd": "markdown"
  },
  "[workbench]": {
    "colorTheme": "Oceanic Next (Sublime)",
    "startupEditor": "none"
  },
  "security.workspace.trust.untrustedFiles": "open",
  "yaml.schemas": {
    "file:///Users/tfarkas/.vscode/extensions/atlassian.atlascode-3.0.3/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "redhat.telemetry.enabled": false,
  "atlascode.bitbucket.enabled": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "typescript.suggest.paths": false,
  "javascript.suggest.paths": false,
  "[quarto]": {
    "editor.suggest.showSnippets": true,
    "editor.snippetSuggestions": "inline",
    "mathjax.theme": "dark"
  },
  "dvc.doNotShowCliUnavailable": true,
  "jupyter.askForKernelRestart": false,
  "[path-autocomplete]": {
    "disableUpOneFolder": false, 
    "pathMappings": {
      "workspace": "${workspace}"
    }
  },
  "[python]": {
    "editor.formatOnType": true,
    "analysis.autoFormatStrings": true
  },
  "[git]": {
    "ignoreMissingGitWarning": true,
    "autofetch": true,
    "terminalAuthentication": false
  },
  "[julia]": {
  "symbolCacheDownload": true,
  "enableTelemetry": false
  }, 
  "[terminal]": {
    "[integrated]": {
      "inheritEnv": false,
      "defaultProfile.linux": "zsh",
      "defaultProfile.osx": "zsh",
      "commandsToSkipShell": [
      "language-julia.interrupt"
      ]
    }
  }, 
  "remote.SSH.connectTimeout": 60,
  "window.zoomLevel": 1
}      

